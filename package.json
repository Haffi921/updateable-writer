{
  "name": "updateable-writer",
  "version": "1.0.0",
  "description": "Updateable Stream-Writer, made for writing lines to stdout and then updating lines with new information",
  "scripts": {
    "build": "rollup --config",
    "test": "tsx test/index.ts"
  },
  "type": "module",
  "keywords": [
    "stdout",
    "updateable",
    "writer",
    "process",
    "terminal"
  ],
  "author": "Hafsteinn Ragnarsson <haffi921@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haffi921/updateable-writer.git"
  },
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      },
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.2.0",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}
